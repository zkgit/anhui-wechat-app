.searchRank{border:none;position: fixed;top: 0;left:0;width: 100%;z-index: 3;}
.searchRank .form{padding-left:20rpx;box-sizing:border-box;width: 100%;}
.searchRank .form .cl{box-sizing:border-box;
width:80%;margin:0;border-radius: 29rpx;padding:0 50rpx 0 20rpx;}
.searchRank .form .cl .jian{width:14rpx;height:8rpx;margin:0 16rpx 6rpx}
.searchRank .form .cl .qx_btn{position:absolute;right:15rpx;top:14rpx;  width:30rpx;height:30rpx;}
.searchRank .form .btn{width:20%;line-height: 58rpx;}
.searchRank .typeBox{background: #49484b;border-radius: 16rpx;width:180rpx;top:95rpx;line-height: 24rpx;}
.searchRank .typeBox:before {
    content: "";
    border-right:20rpx solid transparent;
    border-top: 20rpx solid transparent;
    border-left: 20rpx solid transparent;
    border-bottom: 20rpx solid  #49484b;
    position: absolute;
    top: -35rpx;
    left: 40rpx;
}
.searchRank input{
  height: 58rpx;
  line-height: 58rpx
}
.searchbox{margin-top: 116rpx;}
.searchbox .his{padding: 30rpx 0 30rpx 20rpx}
.searchbox .his image{height: 39rpx;width:38rpx;}
.searchbox .his .hisli{display: inline-block;margin-right:20rpx;margin-bottom:20rpx;white-space: nowrap;padding: 0 40rpx;line-height: 66rpx;height:66rpx;border-radius: 33rpx;}

.searchbox .hot_ul{line-height: 60rpx;}
.searchbox .hot_ul .item{width: 50%;padding:0 10rpx;box-sizing: border-box;}
.searchbox .hot_ul .item .item_box{background: #f1f0f5;border-radius: 10rpx;width: 100%;height:60rpx;}
.searchbox .hot_ul .item .numbtn{width:60rpx;height:60rpx;line-height: 60rpx;background: #999;}
.searchbox .hot_ul .item .first{background:#ff1200; }
.searchbox .hot_ul .item .two{background:#ff7800;}
.searchbox .hot_ul .item .three{background:#ffae00;}
.searchbox .searchText{}
.searchbox .searchText .item{height:88rpx;line-height: 88rpx;}
.searchresult .searchType .item{line-height:24rpx;}
.searchresult .searchType{height:85rpx;}
.searchresult .searchType .c:after{width:30rpx;height:4rpx;background:#ff5f00;border-radius: 2rpx;content:'';display:inline-block;}
.searchresult .dbtype{width:16.66%;height:48rpx;line-height: 48rpx;}
.searchresult .dbtype.active{color:#ff5f00;background: #f7f7f7;border-radius:24rpx;}
.searchresult .list_db{position: fixed;bottom:0;top:249rpx;}
.searchresult .list_db .img,.searchresult .list_zb .img{width:37%;}
.searchresult .list_db .actor,.searchresult .list_zb .actor{height:96rpx;text-overflow: ellipsis;}
.searchresult  .ser_play{height:68rpx;border-radius: 34rpx;line-height:68rpx;padding:0 40rpx;background:#ff5f00;}
.searchresult .ser_play image{width: 29rpx;height:30rpx;vertical-align: middle;}
.searchresult .list_zb{position: fixed;bottom:0;top:201rpx;}
.searchresult .list_zb .channel_info{background: rgba(255,255,255,.65);left:5rpx;bottom:5rpx;top:auto;right:5rpx;border-radius: 4rpx;height:52rpx;line-height: 52rpx;}
.searchresult .list_zb .channel_info image{width:42rpx;height:42rpx;vertical-align:middle;}
.liveresult .item{height:172rpx;}
.liveresult .channel_img{width:132rpx;height:132rpx;}
.liveresult .channel_right{height:152rpx;line-height: 66rpx;margin-left: 152rpx;padding-right: 120rpx;}
.liveresult .channel_play{width:120rpx;height:152rpx;top:0;}
.liveresult .channel_play image{right:40rpx;top:66rpx;width:40rpx;height:41rpx;}



